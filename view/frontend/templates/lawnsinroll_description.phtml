<style>
    #tabs-description {
        font-family: Arial, sans-serif;
        margin: 20px;
    }

    .tabs-navigation {
        list-style: none;
        padding: 0;
        margin: 0;
        display: flex;
        border-bottom: 2px solid #ccc;
        justify-content: center;
    }

    .tab-header {
        margin-right: 5px;
    }

    .tab-title {
        display: block;
        padding: 10px 20px;
        background-color: #f1f1f1;
        border: 1px solid #ccc;
        border-bottom: none;
        text-decoration: none;
        color: #333;
        border-radius: 5px 5px 0 0;
    }

    .tab-header.active .tab-title {
        background-color: #fff;
        border-bottom: 2px solid #fff;
        margin-bottom: -2px;
    }

    .tabs-content {
        border: 1px solid #ccc;
        padding: 20px;
        border-top: none;
    }

    .tabs-content [data-content-type="tab-item"] {
        display: none;
    }

    .tabs-content [data-content-type="tab-item"].active {
        display: block;
    }
</style>

<script>
    document.addEventListener('DOMContentLoaded', function () {
        const tabs = document.querySelectorAll('#tabs-description .tab-header');
        const tabContents = document.querySelectorAll('#tabs-description .tabs-content [data-content-type="tab-item"]');

        // Show the first tab by default
        if (tabs.length > 0) {
            tabs[0].classList.add('active');
            tabContents[0].classList.add('active');
        }

        tabs.forEach((tab, index) => {
            tab.addEventListener('click', (e) => {
                e.preventDefault();

                tabs.forEach(t => t.classList.remove('active'));
                tab.classList.add('active');

                tabContents.forEach(content => {
                    content.classList.remove('active');
                });
                tabContents[index].classList.add('active');
            });
        });
    });
</script>
