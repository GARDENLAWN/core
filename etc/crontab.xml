<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="gardenlawn_base">
        <job name="gardenlawn_core_custom" instance="GardenLawn\Core\Cron\Custom" method="execute">
            <schedule>0 4 * * *</schedule>
        </job>
        <job name="gardenlawn_core_copy_error_pages" instance="GardenLawn\Core\Cron\CopyErrorPages" method="execute">
            <schedule>0 3 * * 0</schedule>
        </job>
    </group>
</config>
